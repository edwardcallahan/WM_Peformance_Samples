<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sakiladb.data.NicerButSlowerFilmList" table="nicer_but_slower_film_list" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.sakiladb.data.NicerButSlowerFilmListId">
            <key-property name="fid" type="short">
                <column name="FID"/>
            </key-property>
            <key-property name="title" type="string">
                <column name="title"/>
            </key-property>
            <key-property name="description" type="string">
                <column name="description" length="65535"/>
            </key-property>
            <key-property name="category" type="string">
                <column name="category" length="25"/>
            </key-property>
            <key-property name="price" type="big_decimal">
                <column name="price" precision="4"/>
            </key-property>
            <key-property name="length" type="short">
                <column name="length"/>
            </key-property>
            <key-property name="rating" type="string">
                <column name="rating" length="5"/>
            </key-property>
            <key-property name="actors" type="string">
                <column name="actors" length="341"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>